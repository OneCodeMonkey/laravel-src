{
  "name": "illuminate/broadcasting",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "psr/log": "^1.0",
    "illuminate/bus": "^6.0",
    "illuminate/contracts": "^6.0",
    "illuminate/queue": "^6.0",
    "illuminate/support": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Illuminate\\Broadcasting\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.x-dev"
